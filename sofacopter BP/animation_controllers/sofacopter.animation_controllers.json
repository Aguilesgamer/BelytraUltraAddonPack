{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.sofacopter": {
      "initial_state": "default",
      "states": {
        "default": {
          "transitions": [
            {
              "fly": "(1.0)"
            }
          ],
          "on_entry": [
            "/function flying"
          ]
        },
        "fly": {
          "transitions": [
            {
              "default": "query.has_rider"
            }
          ],
          "on_entry": [
            "/function flying"
          ]
        }
      }
    }
  }
}
